{
  "breadcrumbs.enabled": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.cursorBlinking": "smooth",
  "editor.foldingStrategy": "indentation",
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 20,
  "editor.fontWeight": "500",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.glyphMargin": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.insertSpaces": true,
  "editor.lineHeight": 40,
  "editor.minimap.enabled": false,
  "editor.mouseWheelScrollSensitivity": 1,
  "editor.mouseWheelZoom": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "none",
  "editor.rulers": [
    80,
    100
  ],
  "editor.showFoldingControls": "mouseover",
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "eslint.enable": true,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "files.defaultLanguage": "markdown",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "javascript.validate.enable": false,
  "path-intellisense.showHiddenFiles": true,
  "prettier.bracketSpacing": true,
  "prettier.jsxBracketSameLine": false,
  "prettier.printWidth": 80,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "none",
  "prettier.useTabs": false,
  "typescript.suggest.enabled": false,
  "typescript.validate.enable": true,
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "window.zoomLevel": 2,
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "activityBar.background": "#1B2B34",
    "activityBar.border": "#0F2026",
    "activityBar.dropBackground": "#0F2026",
    "activityBar.foreground": "#CCC",
    "activityBarBadge.background": "#CCC",
    "activityBarBadge.foreground": "#0F2026",
    "badge.background": "#0F2026",
    "badge.foreground": "#CCC",
    "button.background": "#0F2026",
    "button.foreground": "#CCC",
    "editorGroupHeader.tabsBackground": "#0F2026",
    "input.background": "#1B2B34",
    "sideBar.background": "#0F2026",
    "sideBarSectionHeader.background": "#0F2026",
    "statusBar.background": "#0F2026",
    "statusBar.foreground": "#CCC",
    "statusBar.noFolderBackground": "#0F2026",
    "tab.activeForeground": "#CCC",
    "tab.border": "#0F2026",
    "tab.inactiveBackground": "#0F2026",
    "tab.inactiveForeground": "#CCC",
    "titleBar.activeBackground": "#0F2026",
    "titleBar.activeForeground": "#CCC"
  },
  "workbench.colorTheme": "Oceanic Next",
  "workbench.editor.enablePreview": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.fontAliasing": "default",
  "workbench.iconTheme": "file-icons",
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": true,
  "workbench.tree.indent": 6,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
